# =========================================
# Galaxy Tab S9 WiFi (gts9wifi) - LXC Build
# SM8550 (Snapdragon 8 Gen 2) - Android 14
# =========================================

# KERNEL SOURCE
# Using qlenlen's proven SM8550 kernel (works for S23, should work for Tab S9)
KERNEL_SOURCE=https://github.com/qlenlen/android_kernel_samsung_sm8550
KERNEL_SOURCE_BRANCH=oneui-6.1

# DEFCONFIG
# SM8550 uses this defconfig
KERNEL_CONFIG=vendor/kalama_GKI_defconfig

# ARCHITECTURE
ARCH=arm64

# KERNEL IMAGE
# Check your device tree, usually Image or Image.gz for Samsung
KERNEL_IMAGE_NAME=Image

# CLANG
USE_CUSTOM_CLANG=false
CUSTOM_CLANG_SOURCE=
CUSTOM_CLANG_BRANCH=
CUSTOM_CMDS=

# Google Clang for Android 14
CLANG_BRANCH=master-kernel-build-2022
CLANG_VERSION=r487747c

# GCC
ENABLE_GCC_ARM64=true
ENABLE_GCC_ARM32=true

# EXTRA COMMANDS
EXTRA_CMDS=

# KERNELSU
ENABLE_KERNELSU=true
KERNELSU_TAG=

# LXC/DOCKER SUPPORT (THIS IS WHAT YOU WANT!)
DISABLE_LTO=false
USE_CUSTOM_LXC_PATCH=true
ADD_KPROBES_CONFIG=true
ADD_OVERLAYFS_CONFIG=true
ADD_LXC_CONFIG=true
DISABLE_CC_WERROR=true

# CACHING
ENABLE_CCACHE=true

# DTBO
NEED_DTBO=true

# BOOT IMAGE
BUILD_BOOT_IMG=false
SOURCE_BOOT_IMAGE=
